using system;

namespaces Csharp
{
   class program
   {
       static void Main(string[] args);
       {
           string sayi1 = "1";
           string sayi2 = "3";
           
           int a = int.Parse(sayi1);
           int b = int.Parse(sayi2);
           
           Console.WriteLine(a+b);
           Console.ReadLine();
        }
    }
